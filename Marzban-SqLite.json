        {
          "datasource": {
            "type": "frser-sqlite-datasource",
            "uid": "${DS_FRSER-SQLITE-DATASOURCE}"
          },
          "queryText": "SELECT c.node,\n       COUNT(a.id) AS 'users'\nFROM users AS a\nLEFT JOIN (\n    SELECT\n        user_id,\n        case when n.name = '' then 'Main' else IFNULL(n.name, 'Main') end AS node,\n        ROW_NUMBER() OVER (PARTITION BY user_id ORDER BY node_id) AS row_num\n    FROM node_user_usages AS a\n    LEFT JOIN nodes AS n ON n.id = a.node_id\n    WHERE DATE(a.created_at) = DATE('now')\n        AND strftime('%H', a.created_at) = strftime('%H', 'now')\n) AS c ON c.row_num = 1 AND c.user_id = a.id\nWHERE online_at >= datetime('now', '-1 minute')\nGROUP BY c.node;\n",
          "queryType": "table",
          "rawQueryText": "SELECT c.node,\n       COUNT(a.id) AS 'users'\nFROM users AS a\nLEFT JOIN (\n    SELECT\n        user_id,\n        case when n.name = '' then 'Main' else IFNULL(n.name, 'Main') end AS node,\n        ROW_NUMBER() OVER (PARTITION BY user_id ORDER BY node_id) AS row_num\n    FROM node_user_usages AS a\n    LEFT JOIN nodes AS n ON n.id = a.node_id\n    WHERE DATE(a.created_at) = DATE('now')\n        AND strftime('%H', a.created_at) = strftime('%H', 'now')\n) AS c ON c.row_num = 1 AND c.user_id = a.id\nWHERE online_at >= datetime('now', '-1 minute')\nGROUP BY c.node;\n",
          "refId": "A",
          "timeColumns": []
        }
      ],
      "title": "Connections by Node",
      "type": "bargauge"
